<div class="container">
  <div class="row justify-content-center">
    <div id="dashboard" class="col col-md-10">
      <div class="header">
        <h1 class="text-center mb-2">Dashboard</h1>
      </div>
      <!-- Nav tabs -->
      <div class="d-flex justify-content-center">
        <ul class="nav nav-tabs" id="myTab" role="tablist">
          <li class="nav-dash">
            <a class="nav-link active" id="home-tab" data-toggle="tab" href="#home" role="tab" aria-controls="home" aria-selected="true">My Saved Layouts</a>
          </li>
        </ul>
      </div>
      <hr>

      <% if @project.nil? %>
        <h1 > No saved projects </h1>
        <% else %>
          <div class="m-auto col iframe-container">
            <iframe id="preview" src="<%= preview_path(@project) %>" frameborder="0"></iframe>
          </div>
        <% end %>
    </div>
  </div>
</div>

